{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/microsv-nest/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/microsv-nest/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/microsv-nest",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "rabbit": {
      "type": "library",
      "root": "libs/rabbit",
      "entryFile": "index",
      "sourceRoot": "libs/rabbit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbit/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "language": {
      "type": "library",
      "root": "libs/language",
      "entryFile": "index",
      "sourceRoot": "libs/language/src",
      "compilerOptions": {
        "tsConfigPath": "libs/language/tsconfig.lib.json"
      }
    },
    "policy": {
      "type": "library",
      "root": "libs/policy",
      "entryFile": "index",
      "sourceRoot": "libs/policy/src",
      "compilerOptions": {
        "tsConfigPath": "libs/policy/tsconfig.lib.json"
      }
    },
    "token": {
      "type": "library",
      "root": "libs/token",
      "entryFile": "index",
      "sourceRoot": "libs/token/src",
      "compilerOptions": {
        "tsConfigPath": "libs/token/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "env": {
      "type": "library",
      "root": "libs/env",
      "entryFile": "index",
      "sourceRoot": "libs/env/src",
      "compilerOptions": {
        "tsConfigPath": "libs/env/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "library",
      "root": "libs/notification",
      "entryFile": "index",
      "sourceRoot": "libs/notification/src",
      "compilerOptions": {
        "tsConfigPath": "libs/notification/tsconfig.lib.json"
      }
    }
  }
}