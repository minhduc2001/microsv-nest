{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/libs": [
        "libs/libs/src"
      ],
      "@app/libs/*": [
        "libs/libs/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/rabbit": [
        "libs/rabbit/src"
      ],
      "@app/rabbit/*": [
        "libs/rabbit/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/language": [
        "libs/language/src"
      ],
      "@app/language/*": [
        "libs/language/src/*"
      ],
      "@app/policy": [
        "libs/policy/src"
      ],
      "@app/policy/*": [
        "libs/policy/src/*"
      ],
      "@app/token": [
        "libs/token/src"
      ],
      "@app/token/*": [
        "libs/token/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ],
      "@app/env": [
        "libs/env/src"
      ],
      "@app/env/*": [
        "libs/env/src/*"
      ]
    }
  }
}